<p id="notice"><%= notice %></p>

<h1>Vocabulary Tests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vocabulary_tests.each do |vocabulary_test| %>
      <tr>
        <td><%= link_to 'Show', vocabulary_test %></td>
        <td><%= link_to 'Edit', edit_vocabulary_test_path(vocabulary_test) %></td>
        <td><%= link_to 'Destroy', vocabulary_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

